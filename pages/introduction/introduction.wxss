	font-stretch: normal;
	line-height: 35rpx;
	letter-spacing: 0rpx;
	color: #3793fd;
  
  display: flex;
	justify-content: center;
	align-items: center;
  align-content: center;

  position: absolute;
  left: 24rpx;
}

.containcard .card .info .detail{
  width: 518rpx;
	height: 34rpx;
