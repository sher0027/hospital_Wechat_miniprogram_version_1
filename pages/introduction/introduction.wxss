}

.containcard{
  width: 696rpx;
  background-color: #f7f7f7;
  border-radius: 29rpx;
  position: relative;
  top: 172rpx;
  left: 27rpx;
}
.containcard .title{
	height: 43rpx;
	font-family: Alibaba PuHuiTi;
	font-size: 33rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 35rpx;
	letter-spacing: 0rpx;
  color: #333333;
  
  display: flex;
	align-items: center;
  align-content: center;

  position: absolute;
  top: -81rpx;
  left: 20rpx;
}
